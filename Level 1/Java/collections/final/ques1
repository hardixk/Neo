// You are using Java
import java.util.*;
class Main{
    public static void main(String[]args){
        Scanner s= new Scanner(System.in);
        int n= s.nextInt();
        List<String> arr= new ArrayList<>();
        for(int i=0; i<n;i++){
            arr.add(s.next());
        }
        arr.sort((s1, s2) -> Integer.compare(s1.length(), s2.length()));
        System.out.print(arr);
        
    }
}