import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt();
        char ch = scanner.next().charAt(0);
        
        if (num > 0 && num < 200) {
            int sum = num + (int) ch;
            System.out.println(sum);
        } else {
            System.out.println("Invalid");
        }
    }
}